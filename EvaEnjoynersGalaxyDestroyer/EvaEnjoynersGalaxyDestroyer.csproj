<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <PackageId>GalaxyDestroyer</PackageId>
    <Version>1.0.0</Version>
    <Authors>EvaEnjoyner</Authors>
    <Company>sibFU</Company>
    <Description>An awesome library that does amazing things</Description>
    <PackageTags>awesome utility tools</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryUrl>https://github.com/DaniilPominov/csharp_course25/tree/GalaxyDestroyer</RepositoryUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DocumentationFile>bin\$(Configuration)\$(TargetFramework)\$(PackageId).xml</DocumentationFile>
  </PropertyGroup>

</Project>
